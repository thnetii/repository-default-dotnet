<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
    $CommonMetadataElements$
    <dependencies>
      <dependency id="Microsoft.Build.Tasks.Git" version="$MicrosoftBuildTasksGitDependencyVersion$"/>
      <dependency id="Microsoft.CodeAnalysis.FxCopAnalyzers" version="$MicrosoftCodeAnalysisFxCopAnalyzersDependencyVersion$"/>
      <dependency id="Nullable" version="$NullableDependencyVersion$" />
    </dependencies>
  </metadata>
  <files>
    $CommonFileElements$
    <file src="$ProjectDirectory$\..\..\LICENSE" target="LICENSE" />
    <file src="$ProjectDirectory$\build\*.*" target="build" />
    <file src="$ProjectDirectory$\buildMultiTargeting\*.*" target="buildMultiTargeting" />
  </files>
</package>
